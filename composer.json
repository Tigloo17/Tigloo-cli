{
    "name": "tigloo/cli",
    "description": "Personnal cli for all application.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Tigloo17/Tigloo-cli"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tigloo\\": "src/Tigloo/"
        }
    },
    "authors": [
        {
            "name": "Collard Jean-Michel",
            "email": "collajm@hotmail.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "guzzlehttp/psr7": "^2.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
